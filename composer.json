    }
  ],
  "require": {
    "php": ">=5.6.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "yadakhov/curl": "^1.0",
    "yadakhov/insert-on-duplicate-key": "^0.0.9",
    "yadakhov/laradump": "^1.2",
    "ramsey/uuid": "^3.5"
  },
  "require-dev": {
